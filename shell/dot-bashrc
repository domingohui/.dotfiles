# nix
if [ -e /Users/domingohui/.nix-profile/etc/profile.d/nix.sh ]; then . /Users/domingohui/.nix-profile/etc/profile.d/nix.sh; fi # added by Nix installer

# Load other dotfiles
source $HOME/.bash_aliases
source $HOME/.bash_prompt

# ~/bin
export PATH="$HOME/bin/:$PATH"

# Go
export GOPATH="$HOME/codes/go"
export PATH="$GOPATH/bin:$HOME/bin:$PATH"
